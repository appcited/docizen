{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "./node_modules/.bin/astro dev",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PUBLIC_DOCIZEN_ROOT": "${workspaceFolder}",
      },
      "name": "Development server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "./node_modules/.bin/astro build",
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PUBLIC_DOCIZEN_ROOT": "${workspaceFolder}",
      },
      "name": "Build",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
